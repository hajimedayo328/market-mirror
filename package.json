{
  "name": "market-mirror-db",
  "version": "1.0.0",
  "description": "Market Mirror - Database Final Project",
  "scripts": {
    "demo": "tsx demo-setup.ts",
    "studio": "prisma studio",
    "reset": "prisma migrate reset",
    "vector:setup": "tsx setup-vector.ts",
    "vector:generate": "tsx generate-embeddings.ts",
    "db:check": "tsx quick-restore.ts",
    "db:restore": "npx prisma db seed && npm run demo && npm run vector:generate"
  },
  "dependencies": {
    "@prisma/client": "6.19.1",
    "prisma": "6.19.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "dotenv": "^17.2.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
